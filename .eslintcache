stic/index.js",["247"],"/home/tuanquang/money-management/src/components/ExpenseChart/index.js",[],"/home/tuanquang/money-management/src/components/RingChart/index.js",[],["248","249"],"/home/tuanquang/money-management/src/components/ExpenseHistory/index.js",[],"/home/tuanquang/money-management/src/components/ExpenseBudget/index.js",["250"],"import React from 'react';\nimport { Progress } from 'antd';\nimport {\n  FaGasPump,\n  FaHamburger,\n  FaHome,\n  FaShoppingCart,\n  FaFileInvoiceDollar,\n  FaMusic,\n} from 'react-icons/fa';\n\nconst ExpenseBudget = (props) => {\n  const budget = {\n    Fuel: 5000000,\n    Food: 1800000,\n    Home: 1500000,\n    Shopping: 1000000,\n    Entertainment: 500000,\n    Other: 1000000,\n  };\n  const getIcon = (type) => {\n    switch (type) {\n      case 'Fuel':\n        return <FaGasPump />;\n      case 'Food':\n        return <FaHamburger />;\n      case 'Home':\n        return <FaHome />;\n      case 'Shopping':\n        return <FaShoppingCart />;\n      case 'Entertainment':\n        return <FaMusic />;\n      case 'Other':\n        return <FaFileInvoiceDollar />;\n    }\n  };\n  return (\n    <div className='expense-budget'>\n      {props.expenseReducer.expenseByType.map((item, index) => (\n        <div className='expense-card' key={index}>\n          <Progress\n            type='dashboard'\n            percent={Math.round((item.amount / budget[item.type]) * 100)}\n          />\n          <span className='progress-icon'>{getIcon(item.type)}</span>\n          <p>{item.type}</p>\n          <p>{new Intl.NumberFormat().format(item.amount)}</p>\n        </div>\n      ))}\n    </div>\n  );\n};\nexport default ExpenseBudget;\n","/home/tuanquang/money-management/src/components/MoneyFlow/index.js",[],["251","252"],"/home/tuanquang/money-management/src/components/Modal/TransferModal.js",[],"/home/tuanquang/money-management/src/components/Modal/NewBudgetModal.js",["253"],"/home/tuanquang/money-management/src/components/Modal/ExpensesModal.j